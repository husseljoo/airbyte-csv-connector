{
  "documentationUrl": "https://docs.airbyte.com/integrations/destinations/orange-hdfs",
  "supported_destination_sync_modes": ["overwrite", "append", "append_dedup"],
  "supportsIncremental": true,
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination Orange Hdfs",
    "type": "object",
    "required": ["hdfs_path", "airbyte_host_username", "airbyte_host_password"],
    "additionalProperties": false,
    "properties": {
      "hdfs_path": {
        "type": "string",
        "order": 0,
        "description": "The path in which the data will be saved in hdfs."
      },
      "airbyte_host_username": {
        "type": "string",
        "order": 1,
        "description": "airbyte_host username (used for ssh'ing into host to copyFromLocal into hdfs)"
      },
      "airbyte_host_password": {
        "type": "string",
        "order": 2,
        "airbyte_secret": true,
        "description": "airbyte_host corresponding password"
      },
      "airbyte_host_port": {
        "type": "integer",
        "default": 22,
        "description": "password of user in airbyte_host"
      },
      "consumers_number": {
        "type": "integer",
        "default": 3,
        "description": "Number of consumers that read from the queue."
      }
    }
  }
}
