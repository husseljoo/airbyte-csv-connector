{
  "documentationUrl": "https://docs.airbyte.com/integrations/destinations/orange-hdfs",
  "supported_destination_sync_modes": ["overwrite", "append", "append_dedup"],
  "supportsIncremental": true,
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination Orange Hdfs",
    "type": "object",
    "required": ["hdfs_path"],
    "additionalProperties": false,
    "properties": {
      "hdfs_path": {
        "type": "string",
        "order": 0,
        "description": "The path in which the data will be saved in hdfs."
      },
      "hdfs_file": {
        "type": "string",
        "order": 1,
        "always_show": true,
        "description": "The filename which will be written to the hdfs path. (evaluates to filename if left empty)"
      },
      "variables": {
        "type": "array",
        "order": 2,
        "always_show": true,
        "items": {
          "type": "object",
          "required": ["variable_name", "variable_value"],
          "properties": {
            "variable_name": {
              "type": "string",
              "description": "Name of the variable"
            },
            "variable_value": {
              "type": "string",
              "description": "Name of the variable"
            }
          }
        }
      },
      "airbyte_host_username": {
        "type": "string",
        "order": 2,
        "description": "airbyte_host username (used for ssh'ing into host to copyFromLocal into hdfs)"
      },
      "airbyte_host_password": {
        "type": "string",
        "order": 3,
        "airbyte_secret": true,
        "description": "airbyte_host corresponding password"
      },
      "airbyte_host_port": {
        "type": "integer",
        "default": 22,
        "description": "password of user in airbyte_host"
      },
      "compress": {
        "type": "boolean",
        "default": false,
        "description": "compress files before copying to hdfs",
        "group": "workers"
      },
      "compress_workers_number": {
        "type": "integer",
        "default": 5,
        "description": "number of compress workers number",
        "group": "workers"
      },
      "consumers_number": {
        "type": "integer",
        "default": 5,
        "description": "Number of consumers that copy files to HDFS at once.",
        "group": "workers"
      },
      "producers_number": {
        "type": "integer",
        "default": 5,
        "description": "Number of producers that copy files locally at once.",
        "group": "workers"
      },
      "sftp_max_requests": {
        "type": "integer",
        "default": 128,
        "description": "Maximum number of parallel chunk transfers at once."
      }
    },
    "groups": [
      {
        "id": "workers",
        "title": "Workers"
      }
    ]
  }
}
